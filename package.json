    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.0",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "prisma": "^7.3.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
